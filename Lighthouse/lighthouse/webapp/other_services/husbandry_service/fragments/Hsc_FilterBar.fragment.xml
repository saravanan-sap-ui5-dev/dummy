<core:FragmentDefinition xmlns:uxap="sap.uxap"
    xmlns:table="sap.ui.table"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:vm="sap.ui.comp.variants"
    xmlns:smartMultiInput="sap.ui.comp.smartmultiinput" height="100%">
    <fb:FilterBar showFilterConfiguration = "true" filterBarExpanded="true" clear="onReset" search="onSearch" useToolbar="false" showGoOnFB="true" showClearOnFB="true">
        <fb:filterItems>
            <fb:FilterItem name="A" label="ID">
                <fb:control>
                    <Input/>
                </fb:control>
            </fb:FilterItem>
            <fb:FilterItem name="B" label="Shipping Line">
                <fb:control>
                    <Input/>
                </fb:control>
            </fb:FilterItem>
            <fb:FilterItem name="C" label="Port">
                <fb:control>
                    <MultiComboBox>
                        <core:Item key="1" text="Buraimi" />
                        <core:Item key="2" text="Suwaiq" />
                        <core:Item key="3" text="Sur" />
                        <core:Item key="4" text="Port Sultan Qaboos" />
                        <core:Item key="5" text="Port Sohar" />
                        <core:Item key="6" text="Port Salalah" />
                        <core:Item key="7" text="Port Duqm" />
                        <core:Item key="8" text="Muscat" />
                    </MultiComboBox>
                </fb:control>
            </fb:FilterItem>
            <fb:FilterItem name="D" label="Vessel ID">
                <fb:control>
                    <Input/>
                </fb:control>
            </fb:FilterItem>
            <fb:FilterItem name="E" label="Status">
                <fb:control>
                    <MultiComboBox>
                        <core:Item key="1" text="Draft"/>
                        <core:Item key="2" text="Active"/>
                        <core:Item key="3" text="Inactive"/>
                    </MultiComboBox>
                </fb:control>
            </fb:FilterItem>
        </fb:filterItems>
    </fb:FilterBar>
</core:FragmentDefinition>