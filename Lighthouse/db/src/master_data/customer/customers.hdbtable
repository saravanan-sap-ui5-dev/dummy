COLUMN TABLE "customers" (
	"id" BIGINT  GENERATED BY DEFAULT AS IDENTITY,
	"cardCode" NVARCHAR(256),
	"creditorAccount" NVARCHAR(256),
	"poBox" BIGINT,
	"bayanCode" NVARCHAR(256),
	"shortName" NVARCHAR(256),
	"addressLine1" NVARCHAR(256),
    "addressLine2" NVARCHAR(256),
    "addressLine3" NVARCHAR(256),
	"createdBy" NVARCHAR(256),
	"updatedBy" NVARCHAR(256),
	"remarks" NVARCHAR(500),
	"status" INTEGER  NOT NULL ,
PRIMARY KEY ("id")) UNLOAD PRIORITY 5 AUTO MERGE 