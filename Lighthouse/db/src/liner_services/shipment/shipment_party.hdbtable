COLUMN TABLE "shipment_party" (
    "id" BIGINT  GENERATED BY DEFAULT AS IDENTITY, --primary
	"name" NVARCHAR(256) NOT NULL ,
	"country"  NVARCHAR(256),
	"city"  NVARCHAR(256),
    "phone"  NVARCHAR(256),
    "email"  NVARCHAR(256),
    "fax"  BIGINT,
    "accountNumber"  BIGINT,
    "localOrForeign"  BIGINT,
    "cardCode"  NVARCHAR(256), --foreign
    "customerID"  BIGINT, --foreign
    "shipmentID" BIGINT,
	"status" INTEGER  NOT NULL,
PRIMARY KEY ("id")) UNLOAD PRIORITY 5 AUTO MERGE